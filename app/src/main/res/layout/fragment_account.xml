<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/mettalic_white"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".account.AccountFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="@dimen/_200sdp"
            android:background="@drawable/innooptional3"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Official"
                android:textSize="@dimen/_15sdp"
                android:textStyle="bold"
                android:layout_marginLeft="@dimen/_20sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:textColor="@color/primary"/>


            <TextView
                android:id="@+id/leads"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_30sdp"
                android:text="My Leads"
                android:drawableLeft="@drawable/ic_baseline_leaderboard_24"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginStart="@dimen/_20sdp"
                android:layout_marginEnd="@dimen/_20sdp"
                android:drawablePadding="@dimen/_10sdp"
                android:drawableRight="@drawable/ic_baseline_keyboard_arrow_right_24"
                android:textColor="@color/primary"
                android:textSize="@dimen/_15sdp"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1ssp"
                android:layout_marginStart="@dimen/_20sdp"
                android:layout_marginEnd="@dimen/_20sdp"
                android:background="@color/primary"/>
            <TextView
                android:id="@+id/account_id_card"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_30sdp"
                android:text="ID Card"
                android:drawableLeft="@drawable/ic_baseline_contacts_24"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginStart="@dimen/_20sdp"
                android:layout_marginEnd="@dimen/_20sdp"
                android:drawablePadding="@dimen/_10sdp"
                android:drawableRight="@drawable/ic_baseline_keyboard_arrow_right_24"
                android:textColor="@color/primary"
                android:textSize="@dimen/_15sdp"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1ssp"
                android:layout_marginStart="@dimen/_20sdp"
                android:layout_marginEnd="@dimen/_20sdp"
                android:background="@color/primary"/>


            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Customer Support"
                android:textSize="@dimen/_15sdp"
                android:textStyle="bold"
                android:layout_marginLeft="@dimen/_20sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:textColor="@color/primary"/>


            <TextView
                android:id="@+id/faq_account_inno"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_30sdp"
                android:text="FAQs"
                android:drawableLeft="@drawable/ic_baseline_help_24"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginStart="@dimen/_20sdp"
                android:layout_marginEnd="@dimen/_20sdp"
                android:drawablePadding="@dimen/_10sdp"
                android:drawableRight="@drawable/ic_baseline_keyboard_arrow_right_24"
                android:textColor="@color/primary"
                android:textSize="@dimen/_15sdp"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1ssp"
                android:layout_marginStart="@dimen/_20sdp"
                android:layout_marginEnd="@dimen/_20sdp"
